{
  "name": "@weeknote/cli",
  "version": "1.0.0",
  "description": "CLI tool for Weekly Report Optimizer",
  "type": "module",
  "bin": {
    "weeknote": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc && pnpm build:web",
    "build:web": "cd ../web && pnpm build",
    "lint": "eslint src/",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@weeknote/core": "workspace:*",
    "chalk": "^5.3.0",
    "clipboardy": "^4.0.0",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "inquirer": "^9",
    "open": "^10.1.0",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0"
  },
  "files": [
    "dist",
    "web-dist"
  ]
}
